{
  "environment": "development",
  "version": "1.0.0-dev", 
  "buildTime": "2025-01-24T00:00:00Z",
  "features": {
    "offlineMode": true,
    "progressTracking": true,
    "visualLearning": true,
    "assessmentMode": true,
    "aiRemediation": true,
    "multiUserSupport": true,
    "darkTheme": true,
    "notifications": false
  },
  "supabase": {
    "url": "https://placeholder.supabase.co",
    "anonKey": "placeholder_anon_key_will_be_replaced_in_production",
    "enableRLS": true,
    "schemas": ["public", "student_progress"]
  },
  "openai": {
    "model": "gpt-4o-mini",
    "endpoint": "/api/chat",
    "maxTokens": 1000,
    "temperature": 0.7,
    "triggerMode": "wrong_answers_only"
  },
  "performance": {
    "imageLazyLoading": true,
    "serviceWorker": true,
    "cacheStrategy": "stale-while-revalidate",
    "compressionLevel": 80,
    "maxImageSize": "1200px"
  },
  "accessibility": {
    "wcagLevel": "AA",
    "highContrastMode": true,
    "screenReaderSupport": true,
    "keyboardNavigation": true,
    "minFontSize": "14px"
  },
  "analytics": {
    "enableTracking": false,
    "privacyMode": true,
    "dataRetention": "local_only"
  },
  "security": {
    "contentSecurityPolicy": true,
    "httpsOnly": true,
    "sensitiveDataEncryption": true,
    "sessionTimeout": 3600000
  },
  "deployment": {
    "platform": "github_pages",
    "cdn": "github_cdn",
    "autoDeployment": true,
    "stagingEnvironment": false
  }
}